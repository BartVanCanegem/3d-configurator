{
  "parser": "babel-eslint",
  "rules": {
    "prefer-const": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "one-var": 0,
    "quotes": [
      1,
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "indent": [
      1,
      "tab",
      {"SwitchCase": 1}
    ]
  }
}